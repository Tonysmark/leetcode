{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "semi-spacing": [
            0,
            {
                "before": true,
                "after": false
            }
        ],
        "no-unused-vars": "warn",
        "semi": [
            2,
            "always"
        ]
    }
}